     $(function(){
       var s=[$(window).width()]/1366;
       $('section').css('width',1310*s);
       $('section').css('margin-top',20*s);
       $('#l').css('width',978*s);
       $('#l').css('height',550*s);
       $('#r').css('width',328*s);
       $('#r div').css('width',160*s);
       $('#r div').css('height',90*s);
       $('.imgS').css('width',160*s);
       $('.imgS').css('height',90*s);  
       var arrUrl=['/yc/','/yc/','/yc/','/yc/','/yc/','/yc/','/yc/','/yc/','/yc/','/yc/','/yc/','/yc/'];
       function random(min,max){
         return Math.floor(min+Math.random()*(max-min));
       }        
       var i=random(1,13);
       var oImg=$("<a href='"+arrUrl[i-1]+"'><img class='imgBig' src='/yc/image/index/view"+i+".jpg'/></a>");
       $('#l').append(oImg);
       $('.imgBig').css('width',978*s);
       $('.imgBig').css('height',550*s);

       $('#r div').click(function(){
         var g=$(this).attr('id');
         var oImg=$("<a href='"+arrUrl[g-1]+"'><img class='imgBig' src='/yc/image/index/view"+g+".jpg'/></a>");
         $('#l').empty();
         $('#l').append(oImg);
         $('.imgBig').css('width',978*s);
         $('.imgBig').css('height',550*s);
       })
     })
   </script>

