   <script>
     $(function(){
       function random(min,max){
         return Math.floor(min+Math.random()*(max-min));
       }    
       var oVi=$('video');
       var i=random(0,6);
       oVi.eq(i).show();
       oVi[i].play();
       var s=[$(window).width()]/1353;
       $('section').css('width',1145*s);
       $('section').css('margin-top',30*s);
       var w=978*s,
       h=w*9/16;
       $('video').css('width',w);
       $('video').css('height',h);
       $('#ope').css('width',164*s);
       $('#ope div').css('width',160*s);
       $('#ope div').css('height',90*s);
       $('.imgM').css('width',160*s);
       $('.imgM').css('height',90*s);         
     }) 
   </script>
